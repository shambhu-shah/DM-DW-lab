        "#median\n",
        "element = [1, 2, 3, 4, 5] \n",
        "n = len(element) \n",
        "element.sort() \n",
        "  \n",
        "if n % 2 == 0: \n",
        "    median1 = element[n//2] \n",
        "    median2 = element[n//2 - 1] \n",
        "    median = (median1 + median2)/2\n",
        "else: \n",
        "    median = element[n//2] \n",
        "print(\"Median is: \" + str(median)) "
      ],
